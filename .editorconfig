# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[*.sql]
ij_sql_alias_case = 1
ij_sql_custom_type_case = 2
ij_sql_expr_case_end = 1
ij_sql_from_el_wrap = 0
ij_sql_from_indent_join = false
ij_sql_imp_declare_el_wrap = 1
ij_sql_imp_if_then_wrap_then = true
ij_sql_inset_el_wrap = 0
ij_sql_keep_first_column_comment = false
ij_sql_keyword_case = 1
ij_sql_original_case = false
ij_sql_post_opt_wrap_1 = true